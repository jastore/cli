{
  "name": "jastore",
  "version": "0.0.4",
  "author": "Jastore",
  "bin": {
    "jastore": "./bin/run"
  },
  "bugs": "https://github.com/projets/jastore-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "fs-extra": "^9.0.1",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-localstorage": "^2.1.6",
    "sqlite3": "^5.0.2",
    "tough-cookie": "3.0.1",
    "tough-cookie-filestore": "0.0.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.16",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.17.5",
    "@types/node-localstorage": "^1.3.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/projets/jastore-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jastore",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/initstorage"
    }
  },
  "repository": "projets/jastore-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
